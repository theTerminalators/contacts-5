<h1>Your Contacts</h1>
<% @contacts.each do |contact| %>
  <div>
    <h2><%= link_to contact.full_name, "/contacts/#{contact.id}" %></h2>
    <h3>Email: <%= contact.email %></h3>
    <h3>Phone: <%= contact.japanese_phone %></h3>
    <h3>Updated: <%= contact.display_updated_time %></h3>
  </div>
<% end %>
